{
  "name": "SecureChat",
  "dockerComposeFile": "compose.yaml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "portsAttributes": {
    "80": {
      "label": "Caddy Server"
    },
    "3000": {
      "label": "Next.js Application"
    },
    "27017": {
      "label": "MongoDB Database"
    },
    "9333": {
      "label": "SeaweedFS Master"
    },
    "8080": {
      "label": "SeaweedFS Volume"
    },
    "8888": {
      "label": "SeaweedFS Filer"
    },
    "8333": {
      "label": "SeaweedFS S3 Gateway"
    },
    "4000": {
      "label": "Phoenix Realtime Server"
    },
    "5672": {
      "label": "RabbitMQ Server"
    },
    "1025": {
      "label": "MailHog SMTP Port"
    },
    "8025": {
      "label": "MailHog Web Interface"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "editor.codeActionsOnSave": {
          "source.organizeImports": true
        },
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "[prisma]": {
          "editor.defaultFormatter": "Prisma.prisma"
        },
        "elixirLS.projectDir": "ws-server"
      },
      "extensions": [
        "donjayamanne.git-extension-pack",
        "github.vscode-github-actions",
        "mikestead.dotenv",
        "mutantdino.resourcemonitor",
        "ms-azuretools.vscode-docker",
        "zamerick.vscode-caddyfile-syntax",
        "JakeBecker.elixir-ls",
        "hashicorp.hcl",
        "ms-playwright.playwright",
        "github.vscode-github-actions",
        "hashicorp.terraform",
        "ZainChen.json",
        "esbenp.prettier-vscode",
        "rvest.vs-code-prettier-eslint",
        "Prisma.prisma",
        "christian-kohler.path-intellisense",
        "bradlc.vscode-tailwindcss",
        "pmneo.tsimporter",
        "redhat.vscode-yaml",
        "alexbabichev.formatter-pug"
      ]
    }
  }
}
